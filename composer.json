{
  "name": "vajexal/laravel-attributes-relationships",
  "description": "Laravel eloquent relationships using attributes",
  "keywords": [
    "laravel",
    "eloquent",
    "attribute",
    "relationships"
  ],
  "license": "Unlicense",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^8.7 || ^9.0 || ^10.0 || ^11.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.3",
    "orchestra/testbench": "^8.0",
    "vimeo/psalm": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "Vajexal\\AttributeRelations\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Vajexal\\AttributeRelations\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Vajexal\\AttributeRelations\\AttributeRelationsServiceProvider"
      ]
    }
  }
}
